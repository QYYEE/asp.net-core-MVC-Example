@model List< DD.Electricity.Cloud.AuthServer.Models.DDAccount.DDProjectCheckViewModel>

@{
    Layout = null;
}

@for(var i=0; i < Model.Count; i++)
{
    <div class="row">
        <div class="col-md-12">
            <div>
                <input type="checkbox" style="width:20px; height:20px" name="ckprojects"  asp-for="@Model[i].Checked" class="projectCk" />
                <input type="hidden" asp-for="@Model[i].Text" />
                <input type="hidden" asp-for="@Model[i].Val" class="project" />
                <label style="position:relative;top:-8px;right:-10px" asp-for="@Model[i].Checked">@Model[i].Text</label>
            </div>
        </div>
    </div>
}